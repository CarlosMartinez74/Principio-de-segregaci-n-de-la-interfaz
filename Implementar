// Implementación de un coche tipo Mustang (solo con las funcionalidades básicas)
public class Mustang : IBasicCar
{
    public void Accelerate()
    {
        Console.WriteLine("Speeding up...");
    }

    public void Brake()
    {
        Console.WriteLine("Stopping...");
    }

    public void StartEngine()
    {
        Console.WriteLine("Starting engine...");
    }
}

// Implementación de un coche tipo Tesla ModelS (con características adicionales)
public class ModelS : ITeslaCar
{
    public void Accelerate()
    {
        Console.WriteLine("Speeding up...");
    }

    public void Brake()
    {
        Console.WriteLine("Stopping...");
    }

    public void StartEngine()
    {
        Console.WriteLine("Starting engine...");
    }

    public void LudicrousSpeed()
    {
        Console.WriteLine("wooooooooow...");
    }

    public void AutoPilot()
    {
        Console.WriteLine("Self driving...");
    }
}
